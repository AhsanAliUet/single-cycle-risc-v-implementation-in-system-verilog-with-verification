
addi x6, x0, 5
add x7, x0, x6
add x5, x0, x6
add x3, x0, x6
add x4, x0, x6

addi x1, x0, 1


find:
	add x7, x0, x4 
	add x5, x0, x6    #copy x6 in x5
factorial:
	addi x7, x7, -1
    j multiply

done:
    bne x7, x1, factorial
    j stop


multiply:
	beq x7, x1, stop
	add x6, x6, x5
    addi x7, x7, -1
    bne x7, x1, multiply
    addi x4, x4, -1
    beq x7, x1, find
    
stop:
	j stop